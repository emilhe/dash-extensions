{
  "name": "dash-extensions",
  "version": "0.0.60",
  "description": "Extensions for Plotly Dash.",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:py_and_r": "dash-generate-components ./src/lib/components dash_extensions -p package-info.json --r-prefix ''",
    "build:py_and_r-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:py_and_r",
    "build:py": "dash-generate-components ./src/lib/components dash_extensions -p package-info.json",
    "build_no_r": "npm run build:js && npm run build:py",
    "build:activated": "npm run build:js && npm run build:py_and_r-activated"
  },
  "author": "Emil Eriksen <emil.h.eriksen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "base64-js": "^1.3.1",
    "dompurify": "^2.3.0",
    "mermaid": "^8.11.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "ramda": "^0.26.1",
    "react-before-after-slider": "^1.0.4",
    "react-burger-menu": "^2.6.10",
    "react-loadable": "^5.5.0",
    "react-lottie": "^1.2.3",
    "react-ticker": "^1.2.2",
    "ssri": ">=8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^5.2.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "npm": "^6.14.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-docgen": "^5.4.0",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "webpack": "^5.49.0",
    "webpack-cli": "4.7.2",
    "webpack-serve": "4.0.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
