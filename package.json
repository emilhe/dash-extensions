{
  "name": "dash-extensions",
  "version": "2.0.1",
  "description": "Extensions for Plotly Dash.",
  "main": "index.ts",
  "scripts": {
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/ts/components dash_extensions -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
    "build": "npm run build:js && npm run build:backends && npm run update:pyproject",
    "update:pyproject": "uv run python pyproject.py",
    "watch": "npm run build:js::dev -- --watch"
  },
  "devDependencies": {
    "@plotly/webpack-dash-dynamic-import": "^1.3.0",
    "@types/react": "^17.0.39",
    "css-loader": "^6.7.1",
    "mermaid": "^11.6.0",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.28.0",
    "react": "18.3.1",
    "react-docgen": "^5.4.3",
    "react-dom": "18.3.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "author": "Emil Haldrup Eriksen <emil.h.eriksen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@img-comparison-slider/react": "^8.0.2",
    "dompurify": "^3.2.4",
    "lottie-react": "^2.4.1",
    "sse.js": "^2.6.0"
  }
}
